class Solution {
    static int help(int arr[], int i, int j, int dp[][]){
        if(i>=j) return 0;
        if(dp[i][j] != -1) return dp[i][j];
        int min = Integer.MAX_VALUE;
        for(int k=i; k<j; k++){
            int tmp = help(arr,i,k,dp) + help(arr,k+1,j,dp) + arr[i-1]*arr[j]*arr[k];          
            min = Math.min(tmp,min);     
        }
        return dp[i][j]=min;
    }
    static int matrixMultiplication(int arr[]) {
        // code here
        int dp[][] = new int[arr.length+1][arr.length+1];
        for(int i=0; i<dp.length; i++){
            Arrays.fill(dp[i] ,-1);
        }
        return help(arr,1,arr.length-1,dp);
    }
}
